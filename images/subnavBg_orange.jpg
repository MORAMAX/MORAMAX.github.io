<script type="text/javascript">
    $(function () {

        $(".jsSearchInput").val("Search");

        $(".jsSearchInput").bind("focusin", function () {
            $(".jsSearchInput").val("");
        });

        $(".jsSearchInput").bind("focusout", function () {
            if ($(".jsSearchInput").val() == "") {
                $(".jsSearchInput").val("Search");
            }
        });

        $(".jsSearchInput").bind("keypress", function (e) {
            var searchtext = $(".jsSearchInput").val().replace(/[\s]/, "+");
            if (searchtext == "") { searchtext = "SEARCH"; }
            var code;
            code = (e.keyCode ? e.keyCode : e.which);
            if (code == 13) {
                // force find to occur
                if ($(".jsSearchInput").val().length > 0) {
                    window.location.href = "/search/" + searchtext;
                    e.preventDefault();
                    return false;
                } else {
                    e.preventDefault();
                    return false;
                }
            };

        });

    });
    
</script>
