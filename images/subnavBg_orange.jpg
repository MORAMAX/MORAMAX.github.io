<script type="text/javascript">

    $(function() {

        // show the first image on load
        $(".telephone").children("a").eq(0).show();

        // start the rotation by fading out the current image
        setTimeout(fadeOutImage, 5000);

    });

    var idx = 0;
    function changeImage() {

        // fade in the new image
        $(".telephone").children("a").eq(idx).fadeIn(2000, function() {

            setTimeout(fadeOutImage, 5000);

        });

    }

    function fadeOutImage() {

        if (idx <= $(".telephone").children("a").length - 1) {

            // fade out the image
            $(".telephone").children("a").eq(idx).fadeOut(500, function() {

                // set new index
                idx +=1;
                if (idx > $(".telephone").children("a").length - 1) {
                    idx = 0;
                }

                // fade in the new image
                changeImage()

            });   

        }

    }

</script>

