<script type="text/javascript">
    $(function () {

        $(".jsFinderInputFooter").val("Postcode");

        $(".jsFinderInputFooter").bind("focusin", function () {
            $(".jsFinderInputFooter").val("");
        });

        $(".jsFinderInputFooter").bind("keypress", function (e) {
            var code;
            code = (e.keyCode ? e.keyCode : e.which);
            if (code == 13) {
                // force find to occur
                if ($(".jsFinderInputFooter").val().length > 0) {
                    window.location.href = "/our-showrooms/?pc=" + $(".jsFinderInputFooter").val();
                    e.preventDefault();
                    return false;
                } else {
                    e.preventDefault();
                    return false;
                }
            };

        });

    });
    $(function () {
        //check for cookiepolicy cookie - display message if not set
        if ($.cookie('__popuprab') == null) {
            $.cookie('__popuprab', 1, { expires: 31 });
        } else {
            var intCount = parseInt($.cookie('__popuprab'));
            if (intCount <= 2) {
                intCount += 1;
                if (intCount == 3) {
                    setTimeout(function () {
                        $.fancybox({
                            href: "/request-brochure-form.aspx",
                            width: 780,
                            height: 680,
                            type: "iframe",
                            titlePosition: 'inside',
                            iframe : {
                              preload : false
                            },
                            onClosed: function () {
                                $('iframe').show();
                            }
                        });
                        $.cookie('__popuprab', 100, { expires: 31 });
                    }, 500);
                } else {
                    $.cookie('__popuprab', intCount, { expires: 31 });
                }
            }

        }
    });

    $(".ic-menu").click(function () {
        $("#mainNav").slideToggle();
    });

    $(".ic-phone").click(function () {
        $("#phoneBox").slideToggle();
    });

    function recalculateBannerHeight() {
        var banner = $("#banner");
        banner.height($("#banner > ul > li:first-child").height());
    }

    window.onresize = function(event) {
        recalculateBannerHeight();
    };
    window.onload = function() {
        recalculateBannerHeight();
        if ($(window).width() < 766) {
            $("#mainNav").children("li").each(function () {
                var el = $(this);
                if (el.children("a").attr("href") !== "/videos") {
                    el.children("a").removeAttr('href');
                    el.css("cursor", "pointer");
                    el.click(function () {
                        el.children("ul").slideToggle(event);
                    });
                }
            });
        }
    };

</script>

